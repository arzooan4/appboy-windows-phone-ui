<UserControl x:Class="AppboyUI.Phone.Controls.Slideup"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">

  <UserControl.Resources>
    <ResourceDictionary Source="/AppboyUI.Phone;component/Assets/Styles/Appboy.xaml"/>
  </UserControl.Resources>

  <Border Style="{StaticResource Appboy.Slideup.Border}">
    <Grid Style="{StaticResource Appboy.Slideup.Grid}">
      <TextBlock x:Name="SlideupTextBlock" Style="{StaticResource Appboy.Slideup.TextBlock}"/>
    </Grid>
  </Border>
</UserControl>